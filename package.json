{
  "name": "casarociera",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:ajoli": "ng build --configuration=ajoli",
    "build:otra": "ng build --configuration=otra",
    "deploy:ajoli": "npm run build:ajoli && firebase deploy --only hosting:ajoli",
    "deploy:otra": "npm run build:otra && firebase deploy --only hosting:otra",
    "deploy:all": "npm run deploy:ajoli && npm run deploy:otra"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/fire": "^20.0.0-rc.0",
    "@angular/forms": "^20.1.2",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/router": "^20.1.2",
    "firebase": "^11.10.0",
    "ngx-skeleton-loader": "^11.2.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.3.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
