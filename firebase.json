{
  "hosting": [
     {
      "target": "ajoli",
      "public": "dist/ajoli/browser",
      "rewrites": [{ "source": "**", "destination": "/index.html" }],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "otra",
      "public": "dist/otra/browser",
      "rewrites": [{ "source": "**", "destination": "/index.html" }],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
